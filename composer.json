{
    "name": "phly/phly-requirejs",
    "description": "ZF2 view helper for aggregating RequireJs-compatible require statements",
    "type": "library",
    "license": "BSD-2-Clause",
    "keywords": [
        "zf2",
        "zend",
        "module",
        "requirejs"
    ],
    "homepage": "https://github.com/weierophinney/PhlyRequireJs",
    "authors": [
        {
            "name": "Matthew Weier O'Phinney",
            "email": "matthew@weierophinney.net",
            "homepage": "http://mwop.net/"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zend-view": "~2.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "*@dev",
        "phpunit/PHPUnit": "3.7.*"
    },
    "autoload": {
        "psr-4": {
            "PhlyRequireJs\\": "src/",
            "PhlyRequireJsTest\\": "test/"
        },
        "classmap": [
            "Module.php"
        ]
    }
}
